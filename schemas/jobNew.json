{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "newJob"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "minimum": 0,
            "examples": [
                60000
            ]
        },
        "equity": {
            "type": "number",
            "default": "",
            "title": "The equity Schema",
            "maximum": 1.0,
            "examples": [
                0.05
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "c1"
            ]
        }
    },
    "examples": [
        {
            "title": "newJob",
            "salary": 60000,
            "equity": 0.05,
            "companyHandle": "c1"
        }
    ]
}